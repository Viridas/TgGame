{
	"name": "ToeIIToe",
	"main": "index.html",
	"window": {
		"title": "Toe II Toe",
		"width": 160,
		"height": 144,
		"resizable": true,
		"icon": "www\\icons\\icon-512.png"
	},
	"chromium-args": "--autoplay-policy=no-user-gesture-required --disable-features=HardwareMediaKeyHandling --ignore-gpu-blacklist",
